<italia/onto/CLV/City>
        a                    owl:Class ;
        rdfs:comment         "This class represents the city"@en , "Questa classe rappresenta la città (Comune)"@it ;
        rdfs:isDefinedBy     <italia/onto/CLV> ;
        rdfs:label           "City"@en , "Comune"@it , "Città"@it ;
        rdfs:subClassOf      <italia/onto/CLV/AdminUnitComponent> ;
        rdfs:subClassOf      [ a                            owl:Restriction ;
                               owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                               owl:onClass                  <italia/onto/CLV/Province> ;
                               owl:onProperty               <italia/onto/CLV/hasDirectHigherRank>
                             ] ;
        rdfs:subClassOf      [ a                  owl:Restriction ;
                               owl:allValuesFrom  []  ;
                               owl:onProperty     <italia/onto/CLV/hasHigherRank>
                             ] ;
        rdfs:subClassOf      [ a                  owl:Restriction ;
                               owl:allValuesFrom  <italia/onto/CLV/District> ;
                               owl:onProperty     <italia/onto/CLV/hasDirectLowerRank>
                             ] ;
        owl:versionInfo      "stabile"@it , "stable"@en ;
        prov:wasDerivedFrom  skos:Concept ;
        <italia/onto/CLV/hasRankOrder>  4 ;
        <italia/onto/l0/controlledVocabulary>
                <italia/controlled-vocabulary/territorial-classifications/cities> .
